          "title": {
            "_text": "hololive 主題二創遊戲《HoloCall》於 Steam 公開免費遊玩"
          },
          "link": {
            "_text": "https://www.moelong.com/moelongnews/archives/35771"
          },
          "dc:creator": {
            "_cdata": "萌朧雪猴"
          },
          "pubDate": {
            "_text": "Mon, 10 Mar 2025 16:02:00 +0000"
          },
